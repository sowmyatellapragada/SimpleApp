@{
    ViewData["Title"] = "Home Page";
}

@model IEnumerable<Product>

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Simple App</title>
</head>
<body>
    <ul>
        @foreach (Product p in Model)
        {
            <li>Name: @p.Name, Price: @p.Price</li>
        }
    </ul>
</body>
</html>
